{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "mina": true
  },
  "rules": {
    "react/require-extension": "off",
    "class-methods-use-this": 0,
    "new-cap": ["error", { "capIsNewExceptions": ["Snap"] }]
  }
}
